    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.8",
    "dotenv": "^8.2.0",
    "graphql-tools": "^6.0.9",
    "graphql-yoga": "^1.18.3",
    "merge-graphql-schemas": "^1.7.8",
    "morgan": "^1.10.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  }
